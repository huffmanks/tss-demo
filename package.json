{
  "name": "tss-demo",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -f .env.development -- vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node .output/server/index.mjs",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "db:migrate:dev": "dotenvx run -f .env.development -- drizzle-kit migrate",
    "db:generate:dev": "dotenvx run -f .env.development -- drizzle-kit generate",
    "db:push:dev": "dotenvx run -f .env.development -- drizzle-kit push",
    "db:studio:dev": "dotenvx run -f .env.development -- drizzle-kit studio",
    "db:migrate:prod": "dotenvx run -f .env.production -- drizzle-kit migrate",
    "db:push:prod": "dotenvx run -f .env.production -- drizzle-kit push"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/electric-db-collection": "^0.2.11",
    "@tanstack/react-db": "^0.1.54",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/react-router-ssr-query": "^1.131.7",
    "@tanstack/react-start": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "better-auth": "^1.4.5",
    "drizzle-orm": "^0.45.0",
    "jose": "^6.1.3",
    "lucide-react": "^0.544.0",
    "nitro": "latest",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.0.6",
    "uuid": "^13.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.1",
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/eslint-config": "^0.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "drizzle-kit": "^0.31.8",
    "jsdom": "^27.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "web-vitals": "^5.1.0"
  }
}
